  "version": "0.2.0",
  "publisher": "bridgetamana",
  "license": "MIT",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "apple",
    "macos",
    "sonoma",
    "glass",
    "dark",
    "light",
    "rounded",
    "liquid-glass",
    "dusk",
    "noir"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Dusk Noir Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/dusk-noir-dark.json"
      },
      {
        "label": "Dusk Noir Light",
        "uiTheme": "vs",
        "path": "./themes/dusk-noir-light.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Bridgetamana/vscode-dusk-noir.git"
  }
